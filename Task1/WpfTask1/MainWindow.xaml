        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <TextBox Name="Coordinates" HorizontalAlignment="Left" Height="59" Margin="111,91,0,0" TextWrapping="Wrap" AcceptsReturn="True"  VerticalAlignment="Top" Width="120" TextChanged="Coordinates_TextChanged"/>
        <TextBlock Name="Result" HorizontalAlignment="Left" Margin="358,123,0,0" TextWrapping="Wrap" VerticalAlignment="Top"/>
        <Button Name="Ok" Content="Выполнить" HorizontalAlignment="Left" Margin="261,91,0,0" VerticalAlignment="Top" Width="75" Click="Ok_Click" RenderTransformOrigin="0.493,0.8"/>
        <Button Name="OpenFile" Content="открыть файл" HorizontalAlignment="Left" Margin="111,171,0,0" VerticalAlignment="Top" Width="120" RenderTransformOrigin="0.5,0.5" Click="OpenFile_Click">
        </Button>

    </Grid>
</Window>
